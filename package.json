{
  "name": "nanostore-publisher",
  "version": "0.2.34",
  "description": "Publish UHRP Content with NanoStore",
  "main": "src/index.ts",
  "types": "src/types/types.d.ts",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "doc": "documentation readme src/index.ts --section=API",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test:integration": "jest --config jest.integration.config.js",
    "coverage": "jest --coverage",
    "test:performance": "node performance-tests.js",
    "docs:validate": "documentation lint src/**/*.js",
    "test:e2e": "cypress run",
    "test:compatibility": "node compatibility-tests.js",
    "config:validate": "node validate-config.js",
    "test:custom": "node custom-tests.js",
    "test:all": "npm run lint && npm run type-check && npm run test && npm run test:integration && npm run coverage && npm audit && npm run test:performance && npm run docs:validate && npm run test:e2e && npm run test:compatibility && npm run config:validate && npm run test:custom",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/p2ppsr/nanostore-publisher.git"
  },
  "keywords": [
    "nanostore",
    "UHRP"
  ],
  "author": "Peer-to-peer Privacy Systems Research LLC",
  "license": "Open BSV License",
  "bugs": {
    "url": "https://github.com/p2ppsr/nanostore-publisher/issues"
  },
  "homepage": "https://github.com/p2ppsr/nanostore-publisher#readme",
  "dependencies": {
    "@babbage/sdk-ts": "^0.2.47",
    "authrite-js": "^0.4.34",
    "axios": "^0.21.4",
    "babbage-bsv": "^0.2.5",
    "form-data": "^4.0.0",
    "ninja-base": "^0.3.44",
    "prettier": "^3.3.3",
    "uhrp-url": "^0.1.10"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@types/axios": "^0.14.0",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.3",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "crypto-browserify": "^3.12.0",
    "documentation": "^14.0.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.6.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.88.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  }
}
